        <li>DrugLab</li>
      </recipeUsers>
      <researchPrerequisite>MedicineProduction</researchPrerequisite>
      <skillRequirements>
        <Crafting>3</Crafting>
        <Intellectual>4</Intellectual>
      </skillRequirements>
    </recipeMaker>
  </ThingDef>
</Defs>